{
  "name": "ted-tube",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest docs",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "backend": "json-server --watch db.json --routes routes.json --port 8888 --delay 250 --middlewares ./node_modules/json-server-reset",
    "build-storybook": "build-storybook",
    "dev": "concurrently \"npm run backend\" \"npm run serve\"",
    "storybook": "start-storybook -p 6006",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "core-js": "^3.4.4",
    "material-design-icons-iconfont": "^5.0.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuetify": "^1.5.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@storybook/addon-a11y": "^5.3.9",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-docs": "^5.3.9",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/vue": "^5.3.9",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "concurrently": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "json-server": "^0.15.0",
    "json-server-reset": "^1.1.0",
    "prettier": "^1.19.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.6.10"
  }
